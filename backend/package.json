{
  "name": "tgbot-zapis-backend",
  "version": "1.0.0",
  "description": "Telegram Bot Backend для записи к мастеру маникюра/педикюра",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "prisma migrate deploy && prisma db seed && node dist/index.js",
    "start:prod": "prisma db push --accept-data-loss && node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts"
  },
  "keywords": ["telegram", "bot", "webapp", "booking"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "grammy": "^1.30.0",
    "@grammyjs/types": "^3.14.0",
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "date-fns": "^4.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
